{
  "name": "elements-poc",
  "version": "0.0.0",
  "scripts": {
    "test:it": "ts-node --pretty -P ./tooling/tsconfig.tooling.json ./tooling/test.ts",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ng": "ng",
    "serve:all:demo-angular:control-elements": "npm-run-all --parallel serve:demo-angular:controlled serve:elements:control-elements",
    "serve:all:demo-angular:stand-alone-elements": "npm-run-all --parallel serve:demo-angular:stand-alone serve:elements:stand-alone-elements",
    "serve:all:demo-vanilla:control-elements:hot-elements": "npm-run-all --parallel serve:demo-vanilla:hot-elements-controlled serve:elements:angular-styled",
    "serve:all:demo-vanilla:vanilla-stand-alone:hot-elements": "npm-run-all --parallel serve:demo-vanilla:hot-elements-stand-alone serve:elements:vanilla-stand-alone",
    "serve:all:demo-vanilla:control-elements:bundled-elements": "!!NOTE!!! => serve:demo-vanilla:bundled-elements-controlled and serve:elements:angular-styled:bundled needs to be run in separate consoles :(",
    "serve:all:demo-vanilla:vanilla-stand-alone:bundled-elements": "!!NOTE!!! => serve:demo-vanilla:bundled-elements-stand-alone and serve:elements:stand-alone:bundled needs to be run in separate consoles :(",
    "gzip:all": "gzip-all \"dist/**/*.{css,js,html}\"",
    "generate:bundles": "node ./tooling/elements-bundling.js",
    "build:elements:angular-styled": "npm run compile:tooling && ng build --project elements -c=angularStyled",
    "build:elements:vanilla-stand-alone": "npm run compile:tooling && ng build --project elements -c=vanillaStandAlone",
    "serve:elements:angular-styled": "npm run compile:tooling && ng serve --project elements -c=angularStyled",
    "serve:elements:vanilla-stand-alone": "npm run compile:tooling && ng serve --project elements -c=vanillaStandAlone",
    "serve:elements-special:stand-alone": "npm run compile:tooling && ng serve --project elements-special",
    "serve:demo-angular:control-element": "ng serve --project demo -c=controlElement",
    "serve:demo-angular:stand-alone-element": "ng serve --project demo -c=standAloneElement",
    "build:demo-angular": "ng build --project demo",
    "serve:demo-vanilla:hot-elements-vanilla-stand-alone": "ng s --project demo-vanilla",
    "serve:demo-vanilla:hot-elements-angular-styled": "cd projects/_demo-vanilla &&          live-server --host=localhost --port=4200 --proxy=/element1/:http://localhost:4210/ --open=index.angular-styled.dev.html",
    "serve:demo-vanilla:bundled-elements-vanilla-stand-alone": "cd projects/_demo-vanilla && live-server --host=localhost --port=4200 --proxy=/elements/:http://localhost:4242/ --open=index.vanilla-stand-alone.html",
    "serve:demo-vanilla:bundled-elements-angular-styled": "cd projects/_demo-vanilla &&      live-server --host=localhost --port=4200 --proxy=/elements/:http://localhost:4242/ --open=index.angular-styled.html",
    "compile:tooling": "tsc -p tooling/tsconfig.tooling.json",
    "readme:gen-toc": "markdown-toc ../README.md -i"
  },
  "private": true,
  "dependencies": {
    "@angular-extensions/elements": "8.7.0",
    "@angular/animations": "~8.2.11",
    "@angular/cdk": "~8.2.0",
    "@angular/common": "~8.2.11",
    "@angular/compiler": "~8.2.11",
    "@angular/core": "~8.2.11",
    "@angular/elements": "8.2.11",
    "@angular/forms": "~8.2.11",
    "@angular/material": "8.2.0",
    "@angular/platform-browser": "~8.2.11",
    "@angular/platform-browser-dynamic": "~8.2.11",
    "@angular/router": "~8.2.11",
    "@juristr/ngx-lazy-el": "^0.2.0",
    "@ng-dynamic-forms/core": "^9.0.1",
    "@ng-dynamic-forms/ui-material": "9.0.1",
    "@types/fs-extra": "^8.0.1",
    "@webcomponents/webcomponentsjs": "^2.3.0",
    "angular-element-variants": "0.0.0-beta.2",
    "angular2-text-mask": "9.0.0",
    "application": "^0.1.4",
    "document-register-element": "^1.7.2",
    "ngx-build-plus": "8.1.5",
    "rxjs": "~6.5.3",
    "tslib": "^1.10.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.3",
    "@angular-devkit/build-ng-packagr": "~0.803.3",
    "@angular/cli": "~8.3.3",
    "@angular/compiler-cli": "~8.2.11",
    "@angular/language-service": "~8.2.11",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "concat": "^1.0.3",
    "fs-extra": "^8.1.0",
    "gzip-all": "1.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "live-server": "1.2.1",
    "merge-files": "^0.1.2",
    "ng-packagr": "^5.4.0",
    "npm-run-all": "4.1.5",
    "protractor": "~5.4.0",
    "ts-node": "^7.0.1",
    "tsickle": "^0.37.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  }
}
