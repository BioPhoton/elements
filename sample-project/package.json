{
  "name": "elements-poc",
  "version": "0.0.0",
  "scripts": {
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ng": "ng",
    "serve:all:demo-angular:controlled": "npm-run-all --parallel serve:demo-angular:controlled serve:elements:controlled",
    "serve:all:demo-angular:stand-alone": "npm-run-all --parallel serve:demo-angular:stand-alone serve:elements:stand-alone",
    "serve:all:demo-angular:controlled:zone-less": "npm-run-all --parallel serve:demo-angular:controlled:zone-less serve:elements:controlled:zone-less",
    "serve:all:demo-angular:stand-alone:zone-less": "npm-run-all --parallel serve:demo-angular:stand-alone:zone-less serve:elements:stand-alone:zone-less",
    "serve:all:demo-vanilla:controlled:hot-elements": "npm-run-all --parallel serve:demo-vanilla:hot-elements-controlled serve:elements:controlled",
    "serve:all:demo-vanilla:stand-alone:hot-elements": "npm-run-all --parallel serve:demo-vanilla:hot-elements-stand-alone serve:elements:stand-alone",
    "serve:all:demo-vanilla:controlled:bundled-elements": "!!NOTE!!! => serve:demo-vanilla:bundled-elements-controlled and serve:elements:controlled:bundled needs to be run in separate consoles :(",
    "serve:all:demo-vanilla:stand-alone:bundled-elements": "!!NOTE!!! => serve:demo-vanilla:bundled-elements-stand-alone and serve:elements:stand-alone:bundled needs to be run in separate consoles :(",
    "build:all": "npm-run-all --parallel build:elements:controlled build:elements:stand-alone build:elements:controlled:zone-less build:elements:stand-alone:zone-less",
    "build:all:bundled": "npm-run-all --sequential build:all generate:bundles",
    "generate:bundles": "node ./tooling/elements-bundling.js",
    "build:elements:controlled": "ng build --project elements -c=prodControlled",
    "build:elements:stand-alone": "ng build --project elements -c=prodStandAlone",
    "build:elements:controlled:zone-less": "ng build --project elements -c=prodZoneLessControlled",
    "build:elements:stand-alone:zone-less": "ng build --project elements -c=prodZoneLessStandAlone",
    "serve:elements:controlled": "ng serve --project elements -c=devControlled",
    "serve:elements:stand-alone": "ng serve --project elements -c=devStandAlone",
    "serve:elements:controlled:zone-less": "ng serve --project elements -c=devControlledZoneLess",
    "serve:elements:stand-alone:zone-less": "ng serve --project elements -c=devStandAloneZoneLess",
    "serve:elements:controlled:bundled": "npm run build:elements:controlled && npm run generate:bundles && cd dist/elements-controlled/ && live-server --host=localhost --port=4242 --no-browser",
    "serve:elements:stand-alone:bundled": "npm run build:elements:stand-alone && npm run generate:bundles && cd dist/elements-stand-alone/ && live-server --host=localhost --port=4242 --no-browser",
    "serve:elements-special:stand-alone": "ng serve --project elements-special",
    "serve:demo-angular:controlled": "ng serve --project demo -c=controlled",
    "serve:demo-angular:stand-alone": "ng serve --project demo -c=standAlone",
    "build:demo-angular": "ng build --project demo",
    "serve:demo-angular:controlled:zone-less": "ng serve --project demo -c=controlledZoneLess",
    "serve:demo-angular:stand-alone:zone-less": "ng serve --project demo -c=standAloneZoneLess",
    "serve:demo-angular:with-elements:controlled": "ng serve --project demo -c=withElementsControlled",
    "serve:demo-angular:with-elements:stand-alone": "ng serve --project demo -c=withElementsStandAlone",
    "serve:demo-angular:with-elements:controlled:zone-less": "ng serve --project demo -c=withElementsZoneLessControlled",
    "serve:demo-angular:with-elements:stand-alone:zone-less": "ng serve --project demo -c=withElementsZoneLessStandAlone",
    "serve:demo-vanilla:hot-elements-stand-alone": "cd projects/demo-vanilla &&     live-server --host=localhost --port=4200 --proxy=/elements/:http://localhost:4242/ --open=index.stand-alone.dev.html",
    "serve:demo-vanilla:hot-elements-controlled": "cd projects/demo-vanilla &&      live-server --host=localhost --port=4200 --proxy=/elements/:http://localhost:4242/ --open=index.controlled.dev.html",
    "serve:demo-vanilla:bundled-elements-stand-alone": "cd projects/demo-vanilla && live-server --host=localhost --port=4200 --proxy=/elements/:http://localhost:4242/ --open=index.stand-alone.html",
    "serve:demo-vanilla:bundled-elements-controlled": "cd projects/demo-vanilla &&  live-server --host=localhost --port=4200 --proxy=/elements/:http://localhost:4242/ --open=index.controlled.html",
    "readme:gen-toc": "markdown-toc ../README.md -i"
  },
  "private": true,
  "dependencies": {
    "@angular-extensions/elements": "8.7.0",
    "@angular/animations": "~8.2.4",
    "@angular/cdk": "~8.2.0",
    "@angular/common": "~8.2.4",
    "@angular/compiler": "~8.2.4",
    "@angular/core": "~8.2.4",
    "@angular/elements": "8.2.5",
    "@angular/forms": "~8.2.4",
    "@angular/material": "8.2.0",
    "@angular/platform-browser": "~8.2.4",
    "@angular/platform-browser-dynamic": "~8.2.4",
    "@angular/router": "~8.2.4",
    "@juristr/ngx-lazy-el": "^0.2.0",
    "@ng-dynamic-forms/core": "9.0.1",
    "@ng-dynamic-forms/ui-material": "9.0.1",
    "angular2-text-mask": "9.0.0",
    "application": "^0.1.4",
    "document-register-element": "^1.7.2",
    "ngx-build-plus": "^8.1.4",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.3",
    "@angular-devkit/build-ng-packagr": "~0.803.3",
    "@angular/cli": "~8.3.3",
    "@angular/compiler-cli": "~8.2.4",
    "@angular/language-service": "~8.2.4",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "concat": "1.0.3",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "live-server": "1.2.1",
    "ng-packagr": "^5.4.0",
    "npm-run-all": "4.1.5",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.37.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  }
}
